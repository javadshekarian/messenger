<div class="channelContainer aic frc fww dsn pr" id="channelContainer">
    <i class="fa fa-multiply cc" id="backToMenu"></i>
    <form action="#" class="aic frsb" id="channelForm">
        <div id="level1" class="tran pr">
            <div class="ac ffr capit w170">enter channel name</div>
            <input type="text"  class="channelInput mt10" placeholder="channel name" id="channelName">
            <input type="text" id="channelDiscription" class="channelInput mt10" spellcheck="false" placeholder="discription (optional)">
            <div class="frsb mt10 aic" id="wordNum">
                <div class="cgray fs14">0/50</div>
                <div>
                    <div class="cr fs12 ffr fwb dsn w190">your that's more than 50 character!</div>
                </div>
            </div>
            <input type="file" class="dsn" id="channelPicture">
            <div class="ac ffr capit w170 mt10 fs12">select picture for your channel</div>
            <label for="channelPicture" class="mt10 aic frc" id="channelPictureL">
                <i class="fa fa-plus ac"></i>
            </label>
            <div class="ac ffr m-auto w170 fs10 dsn">you can select just jpg,jpeg,png !</div>
            <div class="frfe w100">
                <button type="button" class="saveContact mt10i" id="gLevel2">next</button>
            </div>
        </div>
        <div id="level2" class="tran fcfs pr">
            <div class="box4">
                <i class="fa fa-arrow-left dib" id="leftLevel1"></i>
                <input type="text" class="channelInput" placeholder="search contact">
            </div>
            <div class="allContactBox fcfs fww">
                
            </div>
            <div class="frfe">
                <button type="submit" class="createChannelSubmit">create channel</button>
            </div>
        </div>
    </form>
    <div id="endOfCreateChannel" class="frc aic dsn">
        <div class="fcc">
            <i class="fa fa-check" aria-hidden="true"></i>
            <svg viewBox="0 0 90 80">
                <text class="svgText" y="40" x="5">created!</text>
            </svg>
        </div>
    </div>
</div>